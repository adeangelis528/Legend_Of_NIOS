module level_rom ( input [6:0]	addr,
						output [19:0]	data
					 );

	parameter ADDR_WIDTH = 7;
   parameter DATA_WIDTH =  32;
	logic [ADDR_WIDTH-1:0] addr_reg;
				
	// ROM definition		
	//Each room contains 20x14 background tiles
	parameter [0:2**ADDR_WIDTH-1][DATA_WIDTH-1:0] ROM = {
		  20'b11111111111111111111,
		  20'b10000000000000000011,
        20'b10000000000000000101,
		  20'b10000000000000001001,
		  20'b10000000000000010001,
		  20'b10000000000000100001,
		  20'b10000111111111000001,
		  20'b10000111111110000001,
		  20'b10000100000000000001,
		  20'b10001000000000000001,
		  20'b10010000000000000001,
		  20'b10100000000000000001,
		  20'b11000000000000000001,
		  20'b11111111111111111111,
        };

	assign data = ROM[addr];

endmodule